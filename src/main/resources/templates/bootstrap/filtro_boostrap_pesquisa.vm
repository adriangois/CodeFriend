                    <input type="text" class="form-control" placeholder=".col-sm-6">
                </div>
                <div class="col-sm-6">
                    <div class="row">
                        <label class="toggle">
                            <input type="checkbox" name="checkbox-toggle" checked="checked">
                            <i data-swchon-text="ON" data-swchoff-text="OFF"></i>Vencidos</label>
