<ui:composition
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
        xmlns:h="http://xmlns.jcp.org/jsf/html"
        xmlns:f="http://xmlns.jcp.org/jsf/core"
        xmlns:p="http://primefaces.org/ui">


    <p:dialog closeOnEscape="true"  id="dlgCreate" widgetVar="dlgCreate" modal="true" resizable="false"  appendTo="@(body)" header="#{label.Titulo_Incluir_${nameClass}}" >
        <h:form id="frmCreate">

            <p:focus context="frmCreate"/>

            <p:panel>
                <h:panelGrid columns="2" >



                </h:panelGrid>
            </p:panel>
            <div role="createBarraBotoes" >
                <p:commandButton value="#{label.Salvar}" 	actionListener="#{${nameClass}Bean.incluir( ${nameClass}Bean.registro )}" ajax="true" process="@form" update="frmPrincipal:frmCreate"/>
                <p:commandButton value="#{label.Voltar}" 	actionListener="#{${nameClass}Bean.cancelar()}" 	oncomplete="PF('dlgCreate').hide()"/>
            </div>
        </h:form>
    </p:dialog>
</ui:composition>
